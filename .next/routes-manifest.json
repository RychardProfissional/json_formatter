{
  "version": 3,
  "pages404": true,
  "appType": "app",
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "priority": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    },
    {
      "source": "/validator.html",
      "destination": "/tools/json-validator",
      "statusCode": 308,
      "regex": "^(?!/_next)/validator\\.html(?:/)?$"
    },
    {
      "source": "/minify.html",
      "destination": "/tools/json-minify",
      "statusCode": 308,
      "regex": "^(?!/_next)/minify\\.html(?:/)?$"
    },
    {
      "source": "/escape.html",
      "destination": "/tools/json-escape",
      "statusCode": 308,
      "regex": "^(?!/_next)/escape\\.html(?:/)?$"
    },
    {
      "source": "/csv.html",
      "destination": "/tools/json-csv",
      "statusCode": 308,
      "regex": "^(?!/_next)/csv\\.html(?:/)?$"
    },
    {
      "source": "/jsonl.html",
      "destination": "/tools/jsonl-prettify",
      "statusCode": 308,
      "regex": "^(?!/_next)/jsonl\\.html(?:/)?$"
    },
    {
      "source": "/sobre.html",
      "destination": "/sobre",
      "statusCode": 308,
      "regex": "^(?!/_next)/sobre\\.html(?:/)?$"
    },
    {
      "source": "/contact.html",
      "destination": "/contato",
      "statusCode": 308,
      "regex": "^(?!/_next)/contact\\.html(?:/)?$"
    },
    {
      "source": "/privacy.html",
      "destination": "/politica-de-privacidade",
      "statusCode": 308,
      "regex": "^(?!/_next)/privacy\\.html(?:/)?$"
    },
    {
      "source": "/terms.html",
      "destination": "/termos-de-uso",
      "statusCode": 308,
      "regex": "^(?!/_next)/terms\\.html(?:/)?$"
    },
    {
      "source": "/sobre/",
      "destination": "/sobre",
      "statusCode": 308,
      "regex": "^(?!/_next)/sobre/(?:/)?$"
    },
    {
      "source": "/contato/",
      "destination": "/contato",
      "statusCode": 308,
      "regex": "^(?!/_next)/contato/(?:/)?$"
    },
    {
      "source": "/politica-de-privacidade/",
      "destination": "/politica-de-privacidade",
      "statusCode": 308,
      "regex": "^(?!/_next)/politica-de-privacidade/(?:/)?$"
    },
    {
      "source": "/termos-de-uso/",
      "destination": "/termos-de-uso",
      "statusCode": 308,
      "regex": "^(?!/_next)/termos-de-uso/(?:/)?$"
    },
    {
      "source": "/blog/",
      "destination": "/blog",
      "statusCode": 308,
      "regex": "^(?!/_next)/blog/(?:/)?$"
    }
  ],
  "headers": [],
  "rewrites": {
    "beforeFiles": [],
    "afterFiles": [],
    "fallback": []
  },
  "dynamicRoutes": [],
  "staticRoutes": [
    {
      "page": "/",
      "regex": "^/(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/(?:/)?$"
    },
    {
      "page": "/_global-error",
      "regex": "^/_global\\-error(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/_global\\-error(?:/)?$"
    },
    {
      "page": "/_not-found",
      "regex": "^/_not\\-found(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/_not\\-found(?:/)?$"
    },
    {
      "page": "/tools",
      "regex": "^/tools(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/tools(?:/)?$"
    }
  ],
  "dataRoutes": [],
  "rsc": {
    "header": "rsc",
    "varyHeader": "rsc, next-router-state-tree, next-router-prefetch, next-router-segment-prefetch",
    "prefetchHeader": "next-router-prefetch",
    "didPostponeHeader": "x-nextjs-postponed",
    "contentTypeHeader": "text/x-component",
    "suffix": ".rsc",
    "prefetchSegmentHeader": "next-router-segment-prefetch",
    "prefetchSegmentSuffix": ".segment.rsc",
    "prefetchSegmentDirSuffix": ".segments",
    "clientParamParsing": false,
    "dynamicRSCPrerender": false
  },
  "rewriteHeaders": {
    "pathHeader": "x-nextjs-rewritten-path",
    "queryHeader": "x-nextjs-rewritten-query"
  }
}